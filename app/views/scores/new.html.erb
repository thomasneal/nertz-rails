<h1>Add new Score</h1>
<%= form_for(@score) do |f| %>
   <p>
      <%= f.label :value %><br />
      <%= f.number_field :value %>
   </p>
   <p>
      <%= f.label :user %><br />
      <%= f.select :user_id, options_from_collection_for_select(@users, 'id', 'name') %>
   </p>
   <p>
      <%= f.submit %>
   </p>
<% end %>
